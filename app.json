{
  "expo": {
    "name": "Triblet",
    "slug": "Triblet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "assets/images/TribletLogo.png",
    "scheme": "triblet",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/TribletLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aathifpm.triblet"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/TribletLogo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.aathifpm.triblet",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/TribletLogo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/TribletLogo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app needs access to your photos to let you select a game image."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "4e9d25d1-5f08-4693-bcc5-adda1f5fd2c1"
      }
    },
    "config": {
      "firebase": {
        "apiKey": "AIzaSyDSp6HSxGlNNe2E8BRNITCU04utLr7x89U",
        "authDomain": "triblet-f8227.firebaseapp.com",
        "projectId": "triblet-f8227",
        "storageBucket": "triblet-f8227.firebasestorage.app",
        "messagingSenderId": "770050130559",
        "appId": "1:770050130559:web:e864c8d8ab45b82e25f54a",
        "measurementId": "G-JCB9VV5FTL"
      }
    }
  }
}